    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@remix-run/serve": "^2.12.1",
    "argon2": "^0.41.1",
    "drizzle-orm": "^0.33.0",
    "isbot": "^4.1.0",
    "jose": "^5.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.9",
